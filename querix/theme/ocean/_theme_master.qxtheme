      <DoStyleAction>
        <SetProperty>
          <PropertyPath>
            <PropertyName>HorizontalAlignment</PropertyName>
          </PropertyPath>
          <PropertyValue>Left</PropertyValue>
        </SetProperty>
      </DoStyleAction>
    </StyleSheet>
  </WithClassFilter> 
  
