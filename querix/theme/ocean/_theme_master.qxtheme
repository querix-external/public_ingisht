  <ElementFilter ElementName="Label">
    <StyleSheet>
      <WithPseudoClassFilter PseudoClassName="Message">
        <StyleSheet>
          <DoStyleAction>
            <SetProperty>
              <PropertyPath>
                <PropertyName>Wrapper</PropertyName>
              </PropertyPath>
              <PropertyValue type="Wrapper" Name="popup" Parameter="5" />
            </SetProperty>
          </DoStyleAction>
        </StyleSheet>
      </WithPseudoClassFilter>
      <WithPseudoClassFilter PseudoClassName="Error">
        <StyleSheet>
          <DoStyleAction>
            <SetProperty>
              <PropertyPath>
                <PropertyName>Wrapper</PropertyName>
              </PropertyPath>
              <PropertyValue type="Wrapper" Name="popupError" Parameter=".qx-focused" />
            </SetProperty>
          </DoStyleAction>
        </StyleSheet>
      </WithPseudoClassFilter>
      <WithPseudoClassFilter>
        <StyleSheet>
          <DoStyleAction>
            <SetProperty>
              <PropertyPath>
                <PropertyName>ForeColor</PropertyName>
              </PropertyPath>
              <PropertyValue type="CustomizedColor" RedColor="253" GreenColor="255" BlueColor="0" Alpha="94" />
            </SetProperty>
          </DoStyleAction>
        </StyleSheet>
      </WithPseudoClassFilter>
    </StyleSheet>
  </ElementFilter>
  
  
  <ElementFilter ElementName="ReportViewerConfig">
    <StyleSheet>
      <ElementIdFilter Identifier="screen">
        <StyleSheet>
          <DoStyleAction>
            <SetProperty>
              <PropertyPath>
                <PropertyName>ViewerType</PropertyName>
              </PropertyPath>
              <PropertyValue>TextViewer</PropertyValue>
            </SetProperty>
          </DoStyleAction>
          <DoStyleAction>
            <SetProperty>
              <PropertyPath>
                <PropertyName>Parameter</PropertyName>
              </PropertyPath>
              <PropertyValue>Console report</PropertyValue>
            </SetProperty>
          </DoStyleAction>
        </StyleSheet>
      </ElementIdFilter>
      <ElementIdFilter Identifier="printer">
        <StyleSheet>
          <DoStyleAction>
            <SetProperty>
              <PropertyPath>
                <PropertyName>ViewerType</PropertyName>
              </PropertyPath>
              <PropertyValue>Print</PropertyValue>
            </SetProperty>
          </DoStyleAction>
          <DoStyleAction>
            <SetProperty>
              <PropertyPath>
                <PropertyName>Parameter</PropertyName>
              </PropertyPath>
              <PropertyValue>.txt</PropertyValue>
            </SetProperty>
          </DoStyleAction>
        </StyleSheet>
      </ElementIdFilter>
      <ElementIdFilter Identifier="new_window">
        <StyleSheet>
          <DoStyleAction>
            <SetProperty>
              <PropertyPath>
                <PropertyName>ViewerType</PropertyName>
              </PropertyPath>
              <PropertyValue>NewWindow</PropertyValue>
            </SetProperty>
          </DoStyleAction>
          <DoStyleAction>
            <SetProperty>
              <PropertyPath>
                <PropertyName>Parameter</PropertyName>
              </PropertyPath>
              <PropertyValue>New Tab</PropertyValue>
            </SetProperty>
          </DoStyleAction>
        </StyleSheet>
      </ElementIdFilter>
      <ElementIdFilter Identifier="download">
        <StyleSheet>
          <DoStyleAction>
            <SetProperty>
              <PropertyPath>
                <PropertyName>ViewerType</PropertyName>
              </PropertyPath>
              <PropertyValue>Download</PropertyValue>
            </SetProperty>
          </DoStyleAction>
          <DoStyleAction>
            <SetProperty>
              <PropertyPath>
                <PropertyName>Parameter</PropertyName>
              </PropertyPath>
              <PropertyValue>.txt</PropertyValue>
            </SetProperty>
          </DoStyleAction>
        </StyleSheet>
      </ElementIdFilter>
    </StyleSheet>
  </ElementFilter>
  
    <ElementFilter ElementName="MenuBar">
    <StyleSheet>
      <ElementIdFilter Identifier="demo">
        <StyleSheet>
          <ChildFilter>
            <StyleSheet>
              <ElementFilter ElementName="MenuCommand">
                <StyleSheet>
                  <WithClassFilter ClassName="classic">
                    <StyleSheet />
                  </WithClassFilter>
                </StyleSheet>
              </ElementFilter>
            </StyleSheet>
          </ChildFilter>
        </StyleSheet>
      </ElementIdFilter>
    </StyleSheet>
  </ElementFilter>
  <ElementFilter ElementName="MenuGroup">
    <StyleSheet>
      <WithClassFilter ClassName="expanded">
        <StyleSheet>
          <DoStyleAction>
            <SetProperty>
              <PropertyPath>
                <PropertyName>ImageId</PropertyName>
              </PropertyPath>
              <PropertyValue type="ResourceId" Uri="{CONTEXT}public/querix/icon/svg/24/ic_arrow_drop_down_white_24px.svg" />
            </SetProperty>
          </DoStyleAction>
          <DoStyleAction>
            <SetProperty>
              <PropertyPath>
                <PropertyName>Wrapper</PropertyName>
              </PropertyPath>
              <PropertyValue type="Wrapper" Name="applyClass" Parameter="[&quot;ui-accordion ui-widget ui-helper-reset&quot;,&#xD;&#xA; {&#xD;&#xA;      &quot;find&quot;:&quot;&gt; a&quot;,&#xD;&#xA;      &quot;add&quot;: &quot;ui-accordion-header ui-state-default ui-accordion-header-active ui-state-active ui-corner-top&quot;&#xD;&#xA;  },&#xD;&#xA;{&#xD;&#xA;      &quot;find&quot;:&quot;&gt; ul&quot;,&#xD;&#xA;      &quot;add&quot;: &quot;ui-accordion-content ui-helper-reset ui-widget-content ui-corner-bottom ui-accordion-content-active&quot;&#xD;&#xA;  }]&#xD;&#xA;" />
            </SetProperty>
          </DoStyleAction>
        </StyleSheet>
      </WithClassFilter>

      <DoStyleAction>
        <SetProperty>
          <PropertyPath>
            <PropertyName>Wrapper</PropertyName>
          </PropertyPath>
          <PropertyValue type="Wrapper" Name="applyClass" Parameter="[&quot;ui-accordion ui-widget ui-helper-reset&quot;,&#xD;&#xA; {&#xD;&#xA;      &quot;find&quot;:&quot;&gt; a&quot;,&#xD;&#xA;      &quot;add&quot;: &quot;ui-accordion-header ui-state-default ui-corner-all&quot;&#xD;&#xA;  }]" />
        </SetProperty>
      </DoStyleAction>
    </StyleSheet>
  </ElementFilter>
  <ElementFilter ElementName="ToolbarButton">
    <StyleSheet>
      <DoStyleAction>
        <SetProperty>
          <PropertyPath>
            <PropertyName>Image</PropertyName>
            <PropertyName>ImagePosition</PropertyName>
          </PropertyPath>
          <PropertyValue>Top</PropertyValue>
        </SetProperty>
      </DoStyleAction>
    </StyleSheet>
  </ElementFilter>
 
  <ElementFilter ElementName="Label">
    <StyleSheet>
      <WithClassFilter ClassName="lycia_display_at">
        <StyleSheet>
          <DoStyleAction>
            <SetProperty>
              <PropertyPath>
                <PropertyName>Wrapper</PropertyName>
              </PropertyPath>
              <PropertyValue type="Wrapper" Name="displayAtRedir" Parameter="[  {&quot;rows&quot;:[23],&quot;to&quot;:&quot;popup&quot;,&quot;parameter&quot;:&quot;f1&quot;},&#xD;&#xA; {&quot;rows&quot;:[24],&quot;to&quot;:&quot;popup&quot;,&quot;parameter&quot;:&quot;f2&quot;} ]" />
            </SetProperty>
          </DoStyleAction>
        </StyleSheet>
      </WithClassFilter>
    </StyleSheet>
  </ElementFilter> 
  
  </StyleSheet>